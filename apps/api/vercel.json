{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm --filter @decisions/shared build",
  "installCommand": "echo 'Install handled in buildCommand'",
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "src/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
