{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**", ".next/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "render": {
      "outputs": ["../../apps/web/public/videos/**"],
      "cache": false
    },
    "render:all": {
      "outputs": ["../../apps/web/public/videos/**"],
      "cache": false
    }
  }
}
